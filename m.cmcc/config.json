{
    "root": "/",
    "js": {},
    "css": {
        "css/app.css": [
            "css/app.css",
            "css/views.css"
        ],
        "css/append.css": [
            "css/append.css"
        ]
    },
    "sprite": {
        "src": "./sprite",
